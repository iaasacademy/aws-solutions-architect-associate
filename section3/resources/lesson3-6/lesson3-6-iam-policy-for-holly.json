{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "BucketActions",
			"Effect": "Allow",
			"Action": [
				"s3:GetObject",
				"s3:ListBucket"
			],
			"Resource": "arn:aws:s3:::ritual-roast-source-code"
		},
		{
			"Sid": "ListBucketsInAccount",
			"Effect": "Allow",
			"Action": "s3:ListAllMyBuckets",
			"Resource": "*"
		}
	]
}